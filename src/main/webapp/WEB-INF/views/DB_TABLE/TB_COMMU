CREATE TABLE TB_COMMU(
    COMMU_NO NUMBER, --게시글 번호,댓글이랑 이어줄 거,시퀀스
    USER_NO NUMBER, --작성자
    USER_ID VARCHAR2(100), --유저 이름
    COMMU_TITLE VARCHAR2(100),  --게시글 제목
    COMMU_CONTENT VARCHAR2(300),  --게시글 내용
    COMMU_SUMMER VARCHAR2(800), 
    COMMU_STATUS VARCHAR2(100),
    COMMU_REGISTRATION DATE DEFAULT SYSDATE,  --등록일
    COMMU_COUNT NUMBER
);